<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="product_template_form_view_dsn_weight_type" model="ir.ui.view">
            <field name="name">product.template.form.view.dsn_weight_type</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view" />
            <field name="type">form</field>
            <field name="arch" type="xml">
                <field name="uom_id" position="after">
                    <separator string="Technical" colspan="2"/>
                    <field name="dsn_weight_type"
                           required="False"
                           placeholder="Peso Tipo"/>

                    <field name="dsn_weight_type_margin"
                           required="False"
                           placeholder="Margen Peso Tipo"/>
                </field>
            </field>
        </record>
    </data>
</openerp>