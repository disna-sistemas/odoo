<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="dsn_company_form_view" model="ir.ui.view">
            <field name="name">dsn.company.form.view</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form" />
            <field name="arch" type="xml">
                <notebook position="inside">
                    <page string="etc">
                        <group colspan="2" col="2">
                            <field name="dsn_logo_docs" widget="image"  class="oe_avatar oe_left">Logo for Documents</field>
                            <field name="dsn_logo_export" widget="image"  class="oe_avatar oe_left">Logo Export Documents</field>
                            <field name="dsn_logo2" widget="image"  class="oe_avatar oe_left">Logo Instituto Naturvita</field>
                            <button name="dsn_button_do_work" confirm="¿Seguro?" colspan="2" string="Asigna países" type="object" icon="gtk-execute" class="oe_inline" />
                        </group>
                    </page>
                </notebook>
            </field>
        </record>

    </data>
</openerp>